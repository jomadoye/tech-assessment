    "jwt-decode": "^2.2.0",
    "lodash": "^4.17.4",
    "mocha": "^3.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "moment": "^2.18.1",
    "mongoose": "^4.9.4",
    "morgan": "^1.8.1",
