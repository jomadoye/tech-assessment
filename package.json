{
  "name": "tech-assessment",
  "version": "1.0.0",
  "description": "tech-assessment",
  "main": "app.js",
  "scripts": {
    "start": "nodemon --watch server --exec babel-node ./bin/www",
    "lint": "node_modules/.bin/esw webpack.config.* client server",
    "db:test:seed": "NODE_ENV=test babel-node ./server/test-utility/seeders/seedDatabase",
    "db:seed": "NODE_ENV=development babel-node ./server/test-utility/seeders/seedDatabase",
    "mocha:testServer": "NODE_ENV=test nyc --reporter=text mocha \"./server/test/**/*.spec.js\" --compilers js:babel-register --timeout 10000",
    "mocha:testClient": "nyc --reporter=text mocha client/test/testSetup.js \"./client/test/**/*.spec.js\"",
    "mocha:testClientWatch": "npm run mocha:testClient -- --watch",
    "db:migrate": "NODE_ENV=development sequelize db:migrate:undo:all && sequelize db:migrate",
    "test:server": "NODE_ENV=test npm run db:test:seed && npm run mocha:testServer",
    "snyk-test": "snyk test && NODE_ENV=test npm run db:test:seed && npm run mocha:testServer",
    "test": "npm-run-all test:server mocha:testClient",
    "postinstall": "npm run db:seed",
    "build": "rm -rf dist/ && babel server -d dist/server && npm run build:client",
    "build:client": "webpack --config webpack.config.prod.js && cat client/index.html > dist/client/index.html",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jomadoye/Tech-Assessment.git"
  },
  "author": "Omadoye Jedidiah",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-istanbul": "^0.12.2",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "babili-webpack-plugin": "^0.1.1",
    "bcrypt": "^1.0.2",
    "body-parser": "^1.17.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "classnames": "^2.2.5",
    "coveralls": "^2.13.0",
    "css-loader": "^0.28.1",
    "dotenv": "^4.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
